{
  "extends": [
    "tslint-config-airbnb",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "import-name": false,
    "jsx-boolean-value": false,
    "no-any": true,
    "ordered-imports": [
      true,
      {
        "import-sources-order": "case-insensitive",
        "named-imports-order": "case-insensitive"
      }
    ],
    "prettier": [
      true,
      {
        "trailingComma": "all",
        "singleQuote": true,
        "printWidth": 120
      }
    ],
    "variable-name": false
  }
}